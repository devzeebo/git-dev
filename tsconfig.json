{
  "compilerOptions": {
    "target": "esnext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "incremental": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "#components/*": [
        "./components/*"
      ],
      "#domain/*": [
        "./domain/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}